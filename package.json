{
  "name": "dd-starter",
  "version": "1.0.0",
  "description": "Delmare Digital starter template for Payload CMS",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "refresh-puck": "pnpm remove @delmaredigital/payload-puck && pnpm add @delmaredigital/payload-puck --registry http://localhost:4873",
    "refresh-ba": "pnpm remove @delmaredigital/payload-better-auth && pnpm add @delmaredigital/payload-better-auth --registry http://localhost:4873",
    "refresh-pagetree": "pnpm remove @delmaredigital/payload-page-tree && pnpm add @delmaredigital/payload-page-tree --registry http://localhost:4873",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build --webpack",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "check": "pnpm exec tsc --noEmit && pnpm lint",
    "ci": "payload migrate && pnpm build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --experimental-https",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "eslint --cache --cache-location .next/cache/eslint/",
    "lint:fix": "eslint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=playwright.config.ts",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@better-auth/passkey": "^1.4.18",
    "@delmaredigital/payload-better-auth": "^0.3.11",
    "@delmaredigital/payload-page-tree": "^0.3.8",
    "@delmaredigital/payload-puck": "^0.6.14",
    "@payloadcms/admin-bar": "^3.75.0",
    "@payloadcms/db-postgres": "^3.75.0",
    "@payloadcms/db-vercel-postgres": "^3.75.0",
    "@payloadcms/live-preview-react": "^3.75.0",
    "@payloadcms/next": "^3.75.0",
    "@payloadcms/plugin-redirects": "^3.75.0",
    "@payloadcms/plugin-search": "^3.75.0",
    "@payloadcms/plugin-seo": "^3.75.0",
    "@payloadcms/richtext-lexical": "^3.75.0",
    "@payloadcms/storage-vercel-blob": "^3.75.0",
    "@payloadcms/ui": "^3.75.0",
    "@puckeditor/core": "^0.21.1",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "better-auth": "^1.4.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.6.1",
    "geist": "^1.5.1",
    "graphql": "^16.12.0",
    "lucide-react": "^0.562.0",
    "next": "^16.1.6",
    "next-sitemap": "^4.2.3",
    "payload": "^3.75.0",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.1",
    "sharp": "^0.34.5",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@next/eslint-plugin-next": "^16.1.6",
    "@playwright/test": "^1.58.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/react": "^16.3.2",
    "@types/escape-html": "^1.0.4",
    "@types/node": "^22.19.9",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.24",
    "copyfiles": "^2.4.1",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^26.1.0",
    "playwright": "^1.58.1",
    "playwright-core": "^1.58.1",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.29.1+sha256.958f3137511c16c30a3102d52229c695311661e0e2ed7af8d4f3c4a43f73335d",
  "engines": {
    "node": ">=20.9.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "bufferutil",
      "esbuild",
      "sharp",
      "unrs-resolver",
      "utf-8-validate"
    ]
  }
}
